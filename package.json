    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "mocha": "^10.0.0",
    "openapi-typescript": "^5.4.1",
    "response-openapi-validator": "^1.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"

    "test": "mocha --timeout 3000 --require ts-node/register ./tests/**/*.test.ts",
    "test-watch": "mocha --timeout 3000 --require ts-node/register --extensions ts,tsx --watch --watch-files src 'tests/**/*.test.{ts,tsx}'"
  },
